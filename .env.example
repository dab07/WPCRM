# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://xxx.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJ...
SUPABASE_SERVICE_ROLE_KEY=eyJ...

# Gemini AI
GEMINI_API_KEY=AIza...

# N8N
NEXT_PUBLIC_N8N_API_KEY=eyJ...
N8N_API_KEY_AUTH_ENABLED=true
N8N_BASE_URL=http://localhost:5678

# WhatsApp - Choose ONE provider
WHATSAPP_PROVIDER=twilio  # Options: 'meta' | 'twilio' | '360dialog' | 'wati' | 'vonage' | 'messagebird'

# Option 1: Meta Cloud API (if you have Meta dev account)
WHATSAPP_PHONE_NUMBER_ID=your_phone_number_id
WHATSAPP_BUSINESS_ACCOUNT_ID=your_business_account_id
WHATSAPP_ACCESS_TOKEN=your_permanent_access_token
WHATSAPP_WEBHOOK_VERIFY_TOKEN=your_random_verify_token
WHATSAPP_API_VERSION=v21.0

# Option 2: Twilio (Recommended - Easiest setup)
TWILIO_ACCOUNT_SID=your_account_sid
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# Option 3: 360Dialog (Official Meta partner)
DIALOG_360_API_KEY=your_api_key
DIALOG_360_PARTNER_ID=your_partner_id

# Option 4: Wati.io (Best for non-technical)
WATI_API_KEY=your_api_key
WATI_API_URL=https://live-server-xxxxx.wati.io

# Option 5: Vonage (Enterprise)
VONAGE_API_KEY=your_api_key
VONAGE_API_SECRET=your_api_secret
VONAGE_WHATSAPP_NUMBER=your_number

# Option 6: MessageBird (International)
MESSAGEBIRD_API_KEY=your_api_key
MESSAGEBIRD_CHANNEL_ID=your_channel_id
